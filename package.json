{
  "name": "yuna-website",
  "description": "Promotional website for Yuna",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@6.32.7",
  "simple-git-hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,html,json,css,scss}": [
      "yarn prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp",
    "serve": "gulp watch",
    "release": "cross-env NODE_ENV=production yarn run build && gulp publish"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "delete": "1.1.0",
    "gh-pages": "3.2.3",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "8.0.0",
    "gulp-clean-css": "4.3.0",
    "gulp-cli": "2.3.0",
    "gulp-if": "3.0.0",
    "gulp-pug": "5.0.0",
    "gulp-sass": "5.1.0",
    "gulp-serve": "^1.4.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "gulp-uglify": "3.0.2",
    "husky": "7.0.4",
    "lint-staged": "12.3.8",
    "prettier": "2.6.2",
    "sass": "1.50.0",
    "simple-git-hooks": "2.7.0",
    "ts-node": "10.7.0",
    "typescript": "4.6.3"
  }
}
