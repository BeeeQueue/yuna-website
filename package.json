  "description": "Promotional website for Yuna",
  "version": "1.0.0",
  "author": "BeeeQueue",
  "devDependencies": {
    "cross-env": "5.2.0",
    "delete": "1.1.0",
    "gh-pages": "2.0.1",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "6.0.0",
    "gulp-clean-css": "4.0.0",
    "gulp-cli": "2.0.1",
    "gulp-if": "2.0.2",
    "gulp-pug": "4.0.1",
    "gulp-sass": "4.0.2",
    "gulp-sourcemaps": "2.6.4",
    "gulp-typescript": "^5.0.0",
    "gulp-uglify": "3.0.1",
    "husky": "1.2.0",
    "lint-staged": "8.1.0",
    "node-sass": "4.11.0",
    "prettier": "1.15.3",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
