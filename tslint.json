{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["node_modules/**"]
  },
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false,
    "no-floating-promises": false
  }
}
